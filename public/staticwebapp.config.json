{
  "routes": [
    {
      "route": "/categories/*",
      "rewrite": "https://demostore-apim.azure-api.net/categories/api/Categories/{*}}"
    },
    {
      "route": "/orders/*",
      "rewrite": "https://demostore-apim.azure-api.net/orders/orders/{*}}"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/.auth/*"]
  },
  "responseOverrides": {
    "401": {
      "rewrite": "/401.html",
      "statusCode": 401
    },
    "403": {
      "rewrite": "/403.html",
      "statusCode": 403
    },
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  }
}